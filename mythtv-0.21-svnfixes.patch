+begin
+  if not MySQLPathRead then begin
+    MySqlPathKeyName := 'SOFTWARE\MySQL AB\MySQL Server 5.0';
+    Result := RegKeyExists(HKLM, MySqlPathKeyName);
+  end;
+end;
