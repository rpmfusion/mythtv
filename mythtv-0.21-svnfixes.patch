+  if DownloadMySqlCheckBox.Checked  and FilesDownloaded and (ConfigureSQL.SelectedValueIndex =0) then
+    Result := true;
+  if MySqlInstalled and (ConfigureSQL.SelectedValueIndex =0)  then
